{
  "name": "agentpulse",
  "version": "0.1.0",
  "description": "Make React apps MCP-controllable. Expose component state and actions to AI agents.",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./server": {
      "import": {
        "types": "./dist/server/index.d.ts",
        "default": "./dist/server/index.js"
      },
      "require": {
        "types": "./dist/server/index.d.cts",
        "default": "./dist/server/index.cjs"
      }
    },
    "./preload": {
      "import": {
        "types": "./dist/electron/preload-index.d.ts",
        "default": "./dist/electron/preload-index.js"
      },
      "require": {
        "types": "./dist/electron/preload-index.d.cts",
        "default": "./dist/electron/preload-index.cjs"
      }
    },
    "./main": {
      "import": {
        "types": "./dist/electron/index.d.ts",
        "default": "./dist/electron/index.js"
      },
      "require": {
        "types": "./dist/electron/index.d.cts",
        "default": "./dist/electron/index.cjs"
      }
    }
  },
  "bin": {
    "agentpulse": "./dist/cli.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist",
    "lint": "biome check src",
    "lint:fix": "biome check --write src",
    "format": "biome format --write src",
    "test": "vitest",
    "test:run": "vitest run",
    "check": "tsc --noEmit && attw --pack --ignore-rules=no-resolution && publint",
    "prepublishOnly": "npm run build && npm run check",
    "prepare": "husky"
  },
  "peerDependencies": {
    "react": ">=18.0.0"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "ws": "^8.18.0",
    "zod": "^3.23.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.18.2",
    "@biomejs/biome": "^2.3.12",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/ws": "^8.5.0",
    "husky": "^9.1.7",
    "publint": "^0.3.17",
    "tsup": "^8.5.1",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  },
  "keywords": [
    "react",
    "mcp",
    "model-context-protocol",
    "ai",
    "automation",
    "testing",
    "agent"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  }
}
